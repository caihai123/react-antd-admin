{
  "name": "react-antd-admin",
  "version": "0.1.0",
  "private": true,
  "description": "这是一个'vue-clean-admin'的简单'react'版本，目前只做了基本的'layout'和动态路由，后期会逐渐添加或完善一些小功能和演示页面，但不会做 tabs 和路由缓存。",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^4.18.0",
    "axios": "^0.26.1",
    "react": "^18.0.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "throttle-debounce": "^3.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "craco start",
    "build": "craco build",
    "test": "craco test"
  },
  "eslintConfig": {
    "plugins": [
      "prettier"
    ],
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "prettier/prettier": "warn",
      "eqeqeq": "error",
      "no-console": "warn",
      "no-debugger": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^1.19.1"
  }
}